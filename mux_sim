S_0x13ef04a20 .scope module, "tb_mux4to1" "tb_mux4to1" 2 1;
 .timescale 0 0;
v0x600001ff01b0_0 .var "I", 3 0;
v0x600001ff0240_0 .var "S", 1 0;
