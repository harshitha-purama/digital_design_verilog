v0x600001ff0360_0 .var/i "i", 31 0;
S_0x13ef04d20 .scope module, "uut" "mux4to1" 2 7, 3 1 0, S_0x13ef04a20;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "I";
    .port_info 1 /INPUT 2 "S";
    .port_info 2 /OUTPUT 1 "Y";
v0x600001ff0000_0 .net "I", 3 0, v0x600001ff01b0_0;  1 drivers
v0x600001ff0090_0 .net "S", 1 0, v0x600001ff0240_0;  1 drivers
v0x600001ff0120_0 .var "Y", 0 0;
E_0x6000038f4e40 .event anyedge, v0x600001ff0090_0, v0x600001ff0000_0;
    .scope S_0x13ef04d20;
T_0 ;
    %wait E_0x6000038f4e40;
    %load/vec4 v0x600001ff0090_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x600001ff0000_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x600001ff0120_0, 0, 1;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x600001ff0000_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x600001ff0120_0, 0, 1;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x600001ff0000_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x600001ff0120_0, 0, 1;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x600001ff0000_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x600001ff0120_0, 0, 1;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x13ef04a20;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x13ef04a20 {0 0 0};
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x600001ff01b0_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600001ff0360_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x600001ff0360_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x600001ff0360_0;
    %parti/s 2, 0, 2;
    %store/vec4 v0x600001ff0240_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 17 "$display", "S=%b -> Y=%b", v0x600001ff0240_0, v0x600001ff02d0_0 {0 0 0};
    %load/vec4 v0x600001ff0360_0;
    %addi 1, 0, 32;
    %store/vec4 v0x600001ff0360_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
