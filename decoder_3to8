v0x600002558090_0 .var "Y", 7 0;
E_0x600000258dc0 .event anyedge, v0x600002558000_0;
    .scope S_0x142e04a90;
T_0 ;
    %wait E_0x600000258dc0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x600002558090_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x600002558000_0;
    %store/vec4 v0x600002558090_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x142e054f0;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x142e054f0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600002558240_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x600002558240_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x600002558240_0;
    %parti/s 3, 0, 2;
    %store/vec4 v0x600002558120_0, 0, 3;
    %delay 10, 0;
    %vpi_call 2 15 "$display", "A=%b -> Y=%b", v0x600002558120_0, v0x6000025581b0_0 {0 0 0};
    %load/vec4 v0x600002558240_0;
    %addi 1, 0, 32;
    %store/vec4 v0x600002558240_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
